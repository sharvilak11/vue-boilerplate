{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "./node_modules/.bin/eslint --ext .vue .",
    "lint-fix": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap-vue": "^2.0.2",
    "core-js": "^2.6.5",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "vue": "^2.6.10",
    "vue-advanced-cropper": "^0.12.5",
    "vue-color": "^2.7.0",
    "vue-cookies": "^1.5.13",
    "vue-flatpickr-component": "^8.1.3",
    "vue-infinite-loading": "^2.4.4",
    "vue-router": "^3.1.3",
    "vue-sweetalert2": "^2.1.1",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@e9ine/eslint-config-vue": "0.0.9-a",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-plugin": "^4.2.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.12.0",
    "path": "^0.12.7",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10",
    "yorkie": "^2.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "globals": {
      "Stripe": false,
      "$": false,
      "FB": false
    },
    "extends": [
      "@e9ine/eslint-config-vue"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ],
    "config.js": [
      "node scripts/verifyUntrackable.js"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
